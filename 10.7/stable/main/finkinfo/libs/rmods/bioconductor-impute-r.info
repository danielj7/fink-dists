Depends: <<
	( %type_raw[rversion] = 3.2 ) r-base%type_pkg[rversion] (>= 3.2.0-2),
	( %type_raw[rversion] = 3.1 ) r-base%type_pkg[rversion] (>= 3.1.3-3),
	( %type_raw[rversion] = 3.0 ) r-base%type_pkg[rversion] (>= 3.0.3-16),
