CompileScript: <<
    perl5.8.1 Makefile.PL LIBS='-L%p/lib' INC='-I%p/include' PERL=/usr/bin/perl5.8.1 PREFIX=%p INSTALLPRIVLIB=%p/lib/perl5/5.8.1 INSTALLARCHLIB=%p/lib/perl5/5.8.1/darwin-thread-multi-2level INSTALLSITELIB=%p/lib/perl5/5.8.1 INSTALLSITEARCH=%p/lib/perl5/5.8.1/darwin-thread-multi-2level INSTALLMAN1DIR=%p/share/man/man1 INSTALLMAN3DIR=%p/share/man/man3 INSTALLSITEMAN1DIR=%p/share/man/man1 INSTALLSITEMAN3DIR=%p/share/man/man3 INSTALLBIN=%p/bin INSTALLSITEBIN=%p/bin INSTALLSCRIPT=%p/bin
