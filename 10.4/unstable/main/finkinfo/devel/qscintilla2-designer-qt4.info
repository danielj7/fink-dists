BuildDepends: <<
  ( %type_pkg[qt] = mac) libpng15,
  ( %type_pkg[qt] = mac) system-openssl-dev,
  libiconv-dev,
  qscintilla2.6-qt4-%type_pkg[qt] (>= %v-2),
  qt4-base-%type_pkg[qt] (>= 4.7.3-1),
<<
Depends: <<
  ( %type_pkg[qt] = mac) libpng15-shlibs,
  libiconv,
  qscintilla2.6-qt4-%type_pkg[qt]-shlibs (>= %v-2),
