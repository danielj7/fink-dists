CompileScript: <<
#!/bin/sh -ev

	export KDE4_PREFIX="%p" KDE4_TYPE="%type_pkg[kde]"
	. %p/sbin/kde4-buildenv.sh
	export PATH="%p/lib/ggz2/bin:$PATH"

	mkdir -p build
	pushd build
		cmake $KDE4_CMAKE_ARGS ..
		make VERBOSE=1
	popd
<<

InstallScript: <<
#!/bin/sh -ev

	pushd build
		make -j1 install/fast DESTDIR="%d"
	popd

	rm -rf %i/lib/%type_pkg[kde]/lib/libkolfprivate.dylib
	rm -rf %i/lib/%type_pkg[kde]/lib/libiris_ksirk.dylib

	if [ -d "%i/lib/%type_pkg[kde]/bin/kfourinline.app" ]; then
		install -d -m 755 "%i/lib/%type_pkg[kde]/bin/kfourinline.app/Contents/Resources"
		mv "%i/lib/%type_pkg[kde]/bin/kwin4.app/Contents/Resources/kwin4.icns" \
			"%i/lib/%type_pkg[kde]/bin/kfourinline.app/Contents/Resources/kfourinline.icns"
	else
		rm -rf %i/lib/%type_pkg[kde]/bin/kwin4.app/Contents/Resources/kwin4.icns
	fi

	mkdir -p %i/share/doc/installed-packages
	touch %i/share/doc/installed-packages/%N
<<
