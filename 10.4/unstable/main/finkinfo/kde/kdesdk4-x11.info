CompileScript: <<
#!/bin/sh -ev

	export KDE4_PREFIX="%p" KDE4_TYPE="%type_pkg[kde]"
	. %p/sbin/kde4-buildenv.sh

	mkdir -p build
	pushd build
		cmake -DPERL_EXECUTABLE:FILEPATH=/usr/bin/perl $KDE4_CMAKE_ARGS ..
		make VERBOSE=1
	popd
<<

InstallScript: <<
#!/bin/sh -ev

	pushd build
		make -j1 install DESTDIR="%d"
	popd

	# private libs
	rm -rf %i/lib/%type_pkg[kde]/lib/libkomparedialogpages.dylib
	rm -rf %i/lib/%type_pkg[kde]/lib/libkomparediff2.dylib
	rm -rf %i/lib/%type_pkg[kde]/lib/libkompareinterface.dylib
	rm -rf %i/lib/%type_pkg[kde]/lib/libantlr.dylib

	mkdir -p %i/share/doc/installed-packages
	touch %i/share/doc/installed-packages/%N
<<
