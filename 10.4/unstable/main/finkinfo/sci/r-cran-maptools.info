Package: r-cran-maptools
Version: 0.7-38
Revision: 3
Description: R Tools for reading and handling spatial objects
Homepage: http://r-spatial.sourceforge.net/
License: GPL
Maintainer:  BABA Yoshihiko <babayoshihiko@mac.com>
Source: http://cran.r-project.org/src/contrib/maptools_%v.tar.gz
Source-MD5: d49a7fad79956b80dc1c3c935d243c27
SourceDirectory: maptools
Depends: r-base (>=2.12.0-1), r-cran-sp
CompileScript: <<
  #!/bin/sh -ev
  export TMPDIR=%b
  pushd ../ && %p/bin/R CMD build --binary maptools
<<
InstallScript: <<
  #!/bin/sh -ev
  mkdir -p %i/lib/R/site-library
  cp -R %b/../*.tgz %i/lib/R/site-library
  cd  %i/lib/R/site-library/ && tar zxvf *.tgz
  rm -f %i/lib/R/site-library/*.tgz
  install_name_tool -id %p/lib/R/site-library/maptools/libs/maptools.dylib %i/lib/R/site-library/maptools/libs/maptools.dylib
<<
Shlibs: <<
  %p/lib/R/site-library/maptools/libs/maptools.dylib 0.0.0 %n (>=0.7-38-1)
<<
