InstallScript: <<
 mkdir -p %i/lib/python2.3/lib-dynload
 cp build/lib.*/_socket.so %i/lib/python2.3/lib-dynload
 cp build/lib.*/_ssl.so %i/lib/python2.3/lib-dynload
