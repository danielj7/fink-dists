BuildDepends: arts-dev (>= 1.1.0-3), kdelibs3-ssl-dev (>= %v-3) | kdelibs3-dev (>= %v-3), kdebase3-ssl-dev (>= %v-3) | kdebase3-dev (>= %v-3), automake (>= 1.6), autoconf25, libtool14, fink (>= 0.10.0), dlcompat (>= 20020929), libjpeg (>= 6b), libpng (>= 1.0.12), libpoll (>= 1.1), qt3 (>= 3.0.5-7)
