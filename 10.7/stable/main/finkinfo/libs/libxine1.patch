+#ifndef CLOCK_REALTIME
+#include <time.h>
+
+#define  CLOCK_REALTIME 0 
+static int clock_gettime(int clk_id /*ignored*/, struct timespec *tp)
+{
+  struct timeval now;
+  
+  int rv = gettimeofday(&now, NULL);
+  
+  if (rv != 0) {
+    return rv;
+  }
+  
+  tp->tv_sec = now.tv_sec;
+  tp->tv_nsec = now.tv_usec * 1000;
+  
+  return 0;
+}
+#endif
