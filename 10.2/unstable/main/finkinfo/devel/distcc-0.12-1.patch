diff -uNbr distcc-0.12/src/Makefile distcc-0.12-built/src/Makefile
--- distcc-0.12/src/Makefile	Wed Dec 31 19:00:00 1969
+++ distcc-0.12-built/src/Makefile	Sat Oct 12 16:41:29 2002
@@ -0,0 +1,118 @@
+# $Header: /data/cvs/distcc/src/Makefile.am,v 1.11 2002/06/21 09:15:57 mbp Exp $
+
+# I guess you could also have a plain Makefile without dependencies,
+# and have the GNUmakefile just link to that.
+
+## Variables
+
+common_obj = trace.o util.o io.o exec.o arg.o rpc.o tempfile.o bulk.o	\
+help.o filename.o lock.o
+
+distcc_obj = distcc.o clinet.o where.o hosts.o strip.o $(common_obj) 	\
+implicit.o
+
+distccd_obj = daemon.o serve.o srvnet.o dopt.o dparent.o $(common_obj)
+
+check_PROGRAMS = h_exten h_issource h_scanargs h_hosts h_argvtostr	\
+h_strip
+
+h_exten_obj = h_exten.o trace.o util.o arg.o filename.o
+h_issource_obj = h_issource.o trace.o util.o arg.o filename.o
+h_scanargs_obj = h_scanargs.o trace.o util.o arg.o filename.o implicit.o
+h_hosts_obj = h_hosts.o trace.o util.o hosts.o
+h_argvtostr_obj = h_argvtostr.o trace.o util.o arg.o filename.o
+h_strip_obj = h_strip.o trace.o util.o arg.o filename.o strip.o
+
+# You might think that distccd ought to be in sbin, because it's a
+# daemon.  It is a grey area.  However, the Linux Filesystem Hierarchy
+# Standard (FHS 2.2) says that sbin is for programs "used exclusively
+# by the system administrator".  
+
+# distccd will often be used by non-root users, and when we support
+# ssh it will be somewhat important that it be found in their default
+# path.  Therefore on balance it seems better to put it in bin/.  
+
+# Package maintainers can override this if absolutely necessary, but I
+# would prefer that they do not. -- mbp
+
+bin_PROGRAMS = distcc distccd
+
+INSTALL = install
+INSTALL_PROGRAM = install 
+INSTALL_DATA = install -m 444
+INSTALL_SCRIPT = install -m 555
+
+SHELL = /bin/sh
+
+srcdir = .
+top_srcdir = ..
+
+prefix = /sw
+exec_prefix = ${prefix}
+
+bindir = ${exec_prefix}/bin
+sbindir = ${exec_prefix}/sbin
+
+CFLAGS = -I/sw/include -fno-common -W -Wall -Wshadow -Wpointer-arith -Wcast-align -MMD
+LDFLAGS = -L/sw/lib -multiply_defined suppress -prebind
+LIBS = -lpopt
+CC = gcc
+CPP = gcc -E
+CPPFLAGS = -no-cpp-precomp -I/sw/include
+
+## rules 
+all: $(bin_PROGRAMS) $(sbin_PROGRAMS)
+
+check_programs: $(check_PROGRAMS) $(bin_PROGRAMS) $(sbin_PROGRAMS)
+
+
+
+## BUILD targets
+
+distcc: $(distcc_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $(distcc_obj) $(LIBS)
+
+distccd: $(distccd_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $(distccd_obj) $(LIBS)	
+
+h_exten: $(h_exten_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $^ $(LIBS)
+
+h_issource: $(h_issource_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $^ $(LIBS)
+
+h_scanargs: $(h_scanargs_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $^ $(LIBS)
+
+h_hosts: $(h_hosts_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $^ $(LIBS)
+
+h_argvtostr: $(h_argvtostr_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $^ $(LIBS)
+
+h_strip: $(h_strip_obj)
+	$(CC) $(CFLAGS) $(LDFLAGS) -o $@ $^ $(LIBS)
+
+
+## CLEAN targets
+
+clean:
+	rm -f $(bin_PROGRAMS) $(check_PROGRAMS) *.o
+
+distclean: clean
+
+maintainer-clean: clean
+
+
+
+## CHECK targets
+
+# Can't directly check here; you need to run it from the parent directory
+
+
+## INSTALL targets
+install: $(bin_PROGRAMS) $(sbin_PROGRAMS)
+	$(INSTALL) -d $(bindir)
+	$(INSTALL_PROGRAM) $(bin_PROGRAMS) $(bindir)
+
+# we don't have any sbin programs at the moment
