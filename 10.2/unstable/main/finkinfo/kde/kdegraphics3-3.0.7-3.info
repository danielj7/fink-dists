BuildDepends: arts-dev (>= 1.1.0-3), kdelibs3-ssl-dev (>= %v-3) | kdelibs3-dev (>= %v-3), kdebase3-ssl-dev (>= %v-3) | kdebase3-dev (>= %v-3), automake (>= 1.6), autoconf25, libtool14, fink (>= 0.10.0), imlib (>= 1.9.10-9), giflib (>= 4.1.0) | libungif (>= 4.1.0), xpdf (>= 1.01), libjpeg (>= 6b), libpng (>= 1.0.12), libtiff (>= 3.5.7-7), qt3 (>= 3.0.5-7), freetype2 (>= 2.0.8), glut (>= 3.7-3)
