Package: bioconductor-annotate-r%type_pkg[rversion]
Type: rversion (3.1 3.0)
Version: 1.42.0
Revision: 1
Description: Annotation for microarrays
Homepage: http://www.bioconductor.org/packages/2.14/bioc/html/annotate.html
License: Artistic
Maintainer: Jack Howarth <howarth.at.fink@gmail.com>
Source: http://www.bioconductor.org/packages/2.14/bioc/src/contrib/annotate_%v.tar.gz
Source-MD5: adb4d30b55d2146711dbe35be3dcb3ef
SourceDirectory: annotate
Depends: r-base%type_pkg[rversion], bioconductor-biobase-r%type_pkg[rversion], bioconductor-annotationdbi-r%type_pkg[rversion] (>= 0.1.15), bioconductor-biocgenerics-r%type_pkg[rversion] (>= 0.1.13), cran-xtable-r%type_pkg[rversion]
BuildDepends: r-base%type_pkg[rversion]-dev
CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes annotate
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD install --library=%i/lib/R/%type_raw[rversion]/site-library annotate
<<
