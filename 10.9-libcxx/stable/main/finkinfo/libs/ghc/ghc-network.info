SourceDirectory: network-%v

BuildDependsOnly: false
Depends: ghc (>= 7.10.3-1), ghc (<< 7.10.4-1)

Shlibs: <<
  @rpath/libHSnetwork-2.6.2.1-A6pE0dLgMEk2GtUa1JtTJQ-ghc7.10.3.dylib 0.0.0 %n (>= 2.6.2.1-1)
