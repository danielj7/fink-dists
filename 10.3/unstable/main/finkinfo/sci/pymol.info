SourceDirectory: %n-%v
Depends: python23, libpng3-shlibs, numeric-py23, tcltk, pmw-py23 (>= 1.2-1), glut-shlibs (>= 3.7-3)
BuildDepends: libpng3, glut (>= 3.7-3), db42, readline (>= 4.3-5), gdbm3, gmp, expat, tcltk-dev, dlcompat-dev
PatchScript: sed 's|@FINKPREFIX@|%p|g' <%a/%n.patch | patch -p1
CompileScript: <<
(cd setup; cp Rules.osx-fink %b/Rules.make)
make
<<
InstallScript: <<
mkdir -p %i/bin
mkdir -p %i/lib/%n
(cd setup; cp pymol.com.osx-fink %i/bin/pymol)
cp -r data modules examples test %i/lib/%n
<<
