diff -uNbr distcc-0.12/Makefile distcc-0.12-built/Makefile
--- distcc-0.12/Makefile	Wed Dec 31 19:00:00 1969
+++ distcc-0.12-built/Makefile	Sat Oct 12 16:38:16 2002
@@ -0,0 +1,358 @@
+# Top-level Makefile(.in) for distcc
+
+# $Header: /data/cvs/distcc/Makefile.in,v 1.78 2002/09/30 03:46:04 mbp Exp $
+
+# Note that distcc no longer uses automake, but this file is still
+# structured in a somewhat similar way.
+
+# Remember that a CVS checkout of this project contains some
+# directories that will not be present in a tarball distribution,
+# including web/.  So, those directories must not be built by regular
+# commands (make all, make clean, make distclean), only by
+# maintainer-* or explicit invocations.
+
+# TODO: Installing info files ought to add them to the info directory,
+# but I don't know any portable way to do this.
+
+## VARIABLES
+
+PACKAGE = distcc
+VERSION = 0.12
+PACKAGE_TARNAME = distcc
+SHELL = /bin/sh
+
+srcdir = .
+top_srcdir = .
+
+prefix = /sw
+exec_prefix = ${prefix}
+
+bindir = ${exec_prefix}/bin
+sbindir = ${exec_prefix}/sbin
+libexecdir = ${exec_prefix}/libexec
+datadir = ${prefix}/share
+sysconfdir = ${prefix}/etc
+sharedstatedir = ${prefix}/com
+localstatedir = ${prefix}/var
+libdir = ${exec_prefix}/lib
+infodir = /sw/share/info
+mandir = /sw/share/man
+includedir = ${prefix}/include
+oldincludedir = /usr/include
+docdir = ${datadir}/doc
+pkgdocdir = $(docdir)/distcc
+
+LIBS = -lpopt 
+
+DESTDIR =
+
+INSTALL = install
+INSTALL_PROGRAM = install 
+INSTALL_DATA = install -m 444
+INSTALL_SCRIPT = install -m 555
+
+pkgdoc_DOCS = AUTHORS COPYING COPYING.FDL NEWS OLDNEWS README \
+	DEPENDENCIES INSTALL 
+
+latte_HTML = web/compared.html web/doc.html web/download.html	\
+web/cvs.html web/news.html web/index.html web/problems.html	\
+web/faq.html web/tested.html web/results.html web/roadmap.html
+
+mkinstalldirs = $(SHELL) $(top_srcdir)/mkinstalldirs
+man1dir = $(mandir)/man1
+man8dir = $(mandir)/man8
+man1_MANS = man/distcc.1 man/distccd.1
+
+linuxdoc_DOCS = linuxdoc/distcc.ps.gz linuxdoc/distcc.pdf
+linuxdoc_INFO = linuxdoc/distcc.info.gz
+manualdocdir = $(pkgdocdir)/manual
+pdbbase = distcc-manual-plucker
+pdbfile = linuxdoc/$(pdbbase).pdb
+
+extradoc_DOCS = doc/distcc-irc.txt doc/results.txt doc/scheduling.txt \
+	survey.txt
+
+DIST_COMMON = Makefile.in install-sh configure configure.ac \
+	config.guess config.sub mkinstalldirs
+
+DIST_PATCHES = patches/distc patches/joerg-proof-build-01.diff \
+	patches/distc++ patches/README.patches \
+	patches/granig_distcc_multi-A-record.diff
+
+TAR = tar
+GZIP = gzip
+GZIP_OPT = -9v
+
+distdir = $(top_builddir)/$(PACKAGE)-$(VERSION)/$(subdir)
+
+distdir = $(PACKAGE_TARNAME)-$(VERSION)
+tarball = $(PACKAGE_TARNAME)-$(VERSION).tar.gz
+tarball_sig = $(tarball).asc
+distnews = $(PACKAGE_TARNAME)-$(VERSION).NEWS
+
+SOURCES = src/arg.c src/bulk.c src/clinet.c src/daemon.c src/distcc.c	\
+src/dopt.c src/dparent.c src/exec.c src/filename.c src/h_argvtostr.c	\
+src/h_exten.c src/h_hosts.c src/h_issource.c src/h_scanargs.c		\
+src/help.c src/hosts.c src/io.c src/rpc.c src/serve.c src/srvnet.c	\
+src/tempfile.c src/trace.c src/util.c src/where.c src/zip.c src/strip.c \
+src/h_strip.c src/implicit.c src/lock.c
+
+HEADERS = src/clinet.h src/distcc.h src/exitcode.h src/hosts.h		\
+	src/io.h src/tempfile.h	src/strip.h src/implicit.h		\
+	src/opt.h src/rpc.h src/trace.h src/util.h src/bulk.h 		\
+	src/exec.h src/lock.h
+
+MEN = man/distcc.1 man/distccd.1
+
+
+## OVERALL targets
+
+## IMPLICIT BUILD rules
+
+%.html: %.latte
+	-rm -f $@
+	latte-html -l web/style.latte -o $@ $<
+
+%.gz: %
+	gzip -9vf $<
+
+## BUILD targets
+
+## NOTE: "all" must be the first (default) rule, aside from patterns.
+
+# We don't build the web pages or manual by default, because many
+# people will not have the tools to do it.  Just use all-web or
+# all-linuxdoc if you want them.
+all: 
+	for s in src; do $(MAKE) -C $$s all  || exit $$? ; done
+
+all-web: all-latte
+all-latte: $(latte_HTML)
+
+# TODO: perhaps ought to also build and upload a tarball of HTML files.
+
+all-linuxdoc: linuxdoc/html/distcc.html $(linuxdoc_DOCS)
+
+
+
+## DIST targets
+
+# The sub-targets copy (and if necessary, build) various files that
+# have to go into the tarball.  They also create necessary directories
+# -- bear in mind that they might be run in parallel.
+
+# This looks a bit gross to me, but it's not as bad as it might be :-/
+
+# I was going to try building a .bz2, but it turns out that for the
+# current contents, it's not much better (~54% vs ~56%).  Probably
+# this is because of the big compressed document files.
+
+dist: dist-linuxdoc dist-pkgdoc dist-extradoc dist-main dist-src \
+	dist-python dist-men dist-patches
+	$(TAR) cfz $(tarball) $(distdir)
+	rm -r $(distdir)
+	cp NEWS $(distnews)
+
+dist-sign:
+	gpg -a --detach-sign $(tarball)
+
+dist-men: $(MEN)
+	mkdir -p $(distdir)/man
+	cp $^ $(distdir)/man
+
+dist-linuxdoc: all-linuxdoc linuxdoc/html/distcc.html $(linuxdoc_DOCS) $(linuxdoc_INFO) linuxdoc/distcc.sgml
+	mkdir -p $(distdir)/linuxdoc/html
+	cp $(linuxdoc_DOCS) $(linuxdoc_INFO) linuxdoc/distcc.sgml $(distdir)/linuxdoc
+	cp linuxdoc/html/*  $(distdir)/linuxdoc/html
+
+dist-pkgdoc: $(pkgdoc_DOCS) 
+	mkdir -p $(distdir)
+	cp $^ $(distdir)
+
+dist-extradoc: $(extradoc_DOCS)
+	mkdir -p $(distdir)/doc
+	cp $^ $(distdir)/doc
+
+dist-main: $(DIST_COMMON)
+	mkdir -p $(distdir)
+	cp $^ $(distdir)
+
+dist-src: $(SOURCES) $(HEADERS) src/Makefile.in src/config.h.in
+	mkdir -p $(distdir)/src
+	cp $^ $(distdir)/src
+
+dist-python: python/comfychair.py  test/testdistcc.py
+	mkdir -p $(distdir)/python 
+	mkdir -p $(distdir)/test
+	cp $(srcdir)/python/comfychair.py $(distdir)/python/ 
+	cp $(srcdir)/test/testdistcc.py $(distdir)/test/
+
+dist-patches: $(DIST_PATCHES)
+	mkdir -p $(distdir)/patches
+	cp $^ $(distdir)/patches
+
+
+## BUILD (linuxdoc) targets
+linuxdoc/html/distcc.html: $(srcdir)/linuxdoc/distcc.sgml
+	mkdir -p linuxdoc/html
+	cd linuxdoc/html && linuxdoc --footer=../footer.html -B html ../distcc.sgml
+
+linuxdoc/distcc.ps.gz: linuxdoc/distcc.sgml
+	cd linuxdoc && linuxdoc -B latex -o ps -p a4 distcc.sgml 
+	$(GZIP) $(GZIP_OPT) -f linuxdoc/distcc.ps
+
+linuxdoc/distcc.pdf: linuxdoc/distcc.sgml
+	cd linuxdoc && linuxdoc -B latex -o pdf -p a4 distcc.sgml
+
+linuxdoc/distcc.info.gz: linuxdoc/distcc.sgml
+	cd linuxdoc && linuxdoc -B info distcc.sgml
+	$(GZIP) $(GZIP_OPT) -f linuxdoc/distcc.info
+
+$(pdbfile): linuxdoc/html/distcc.html
+	plucker-build -N 'distcc Manual' --stayonhost -f $(pdbbase) -M 3 $<
+	mv ~/.plucker/$(pdbbase).pdb $(pdbfile)
+
+
+## BUILD (web) targets
+$(latte_HTML): web/style.latte
+
+
+
+## CHECK targets
+
+check_programs:
+	for s in src; do $(MAKE) -C $$s check_programs || exit $$?; done
+
+check: check_programs
+	PYTHONPATH=$(srcdir)/python PATH=`pwd`/src:$$PATH python2.2 $(srcdir)/test/testdistcc.py
+
+run_pyunit:
+	PYTHONPATH=$(srcdir)/python PATH=./src:$$PATH python2.2 $(srcdir)/pyunit/testdistcc.py -v suite
+
+
+
+## CLEAN targets
+
+clean: 
+	for s in src; do $(MAKE) -C $$s clean || exit $$?; done 
+
+maintainer-clean: distclean maintainer-clean-web maintainer-clean-linuxdoc \
+	maintainer-clean-src maintainer-clean-autoconf
+
+# This rule at the moment is a bit wimpy -- it doesn't delete
+# "configure" and co, because they're kept in CVS for distcc, to ease
+# building on machines with bad versions.
+maintainer-clean-autoconf:
+
+maintainer-clean-src:
+	for s in src; do $(MAKE) -C $$s maintainer-clean || exit $$?; done 
+
+maintainer-clean-web: 
+	rm -f $(latte_HTML)
+
+# We only remove the documentation files for maintainer-clean, because
+# many users will not have the tools to rebuild them.  They're shipped
+# up-to-date in the distribution.
+maintainer-clean-linuxdoc: 
+	cd linuxdoc && rm -f distcc.ps distcc.html distcc-*.html \
+	html/* html/* distcc.info distcc.info.gz distcc.ps.gz distcc.pdf
+
+
+distclean-autoconf:
+	rm -f Makefile config.status src/config.h config.cache config.log 
+
+distclean: distclean-autoconf
+	for s in src; do $(MAKE) -C $$s distclean || exit $$?; done 
+
+
+## MAINTAINER targets
+
+always_ChangeLog:
+	cvs2cl --distributed --ignore ChangeLog --revisions --tags -U ./cvs2cl.ufile
+
+upload: upload-web upload-linuxdoc
+
+upload-web: all-web
+	rsync -avz web/ survey.txt \
+		--exclude CVS --exclude '*~' --exclude '*.latte' \
+		samba.org:/home/httpd/distcc/
+
+upload-linuxdoc: all-linuxdoc
+	rsync -azv linuxdoc/ --exclude CVS --exclude '*~' \
+		samba.org:/home/httpd/distcc/manual/
+
+upload-dist:
+	rsync -avP $(tarball) $(distnews) $(tarball_sig) samba.org:/home/ftp/pub/distcc/
+
+## CVSPLOT
+
+cvsplot:
+	cvsplot -cvsdir . \
+		-include '\.(c|h|py)$$' \
+		-linedata /tmp/linedata.txt \
+		-gnuplotlinedata web/cvslinedata.png \
+		-filedata /tmp/filedata.txt \
+		-gnuplotfiledata web/cvsfiledata.png
+
+
+## INSTALL targets
+
+showpaths:
+	@echo "'make install' will install distcc as follows:"
+	@echo "  man pages            $(man1dir)"
+	@echo "  info                 $(infodir)"
+	@echo "  documents            $(pkgdocdir)"
+	@echo "  programs             $(bindir)"
+
+install: showpaths install-doc install-man install-src install-linuxdoc
+
+install-src:
+	cd src && $(MAKE) install
+
+install-man: $(man1_MANS)
+	$(INSTALL) -d $(man1dir)
+	$(INSTALL_DATA) $(man1_MANS) $(man1dir)
+
+install-doc: $(pkgdoc_DOCS)
+	$(INSTALL) -d $(pkgdocdir)
+	$(INSTALL_DATA) $(pkgdoc_DOCS) $(pkgdocdir)
+
+# XXX: install-linuxdoc does not depend on the files it installs,
+# because some people won't have the sgml-tools to do it.  We try to
+# install the pregenerated ones.
+
+install-linuxdoc: $(linuxdoc_DOCS) linuxdoc/html/distcc.html $(linuxdoc_INFO)
+	$(INSTALL) -d $(manualdocdir)/html
+	$(INSTALL) -d  $(infodir)
+	$(INSTALL_DATA) $(linuxdoc_DOCS) $(manualdocdir)
+	$(INSTALL_DATA) $(linuxdoc_INFO) $(infodir)
+	$(INSTALL_DATA) linuxdoc/html/* $(manualdocdir)/html
+
+## LINBOT targets
+
+# This target checks the website for broken links.  It's probably not
+# very interesting unless you are a maintainer and therefore in a
+# position to fix them.
+
+linbot-run:
+	mkdir -p linbot-report
+	linbot -o linbot-report http://distcc.samba.org/
+
+## ANALOG targets
+
+# These are probably only interesting for maintainers
+analog-all:
+	$(MAKE) analog-download
+	$(MAKE) analog-resolve
+	$(MAKE) analog-run
+
+analog-download:
+	rsync -avz 'samba.org:/var/log/httpd/distcc/*_log*' analog/
+
+analog-run:
+	mkdir -p analog/report && cd analog && analog -G +gdistcc.analog
+
+analog-resolve:
+	cat analog/access_log* | \
+		jdresolve --recursive --database analog/jdresolve.db - \
+		> analog/resolved_log
