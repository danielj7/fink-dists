Package: qgis%type_pkg[version]-plugin-manager
Type: version (. 1.6)
Version: 1.0
Revision:1
Description: GNU R plugin to QGIS
Homepage: http://www.ftools.ca/manageR/
License: GPL
Maintainer:  BABA Yoshihiko <babayoshihiko@mac.com>
Source: http://www.ftools.ca/tools/current/manageR.zip
Source-MD5: a6be403c3c10765a81a6510f6c2cff4c
SourceDirectory: manageR
Depends: <<
  qgis%type_pkg[version]-mac | qgis%type_pkg[version]-mac-pgsql | qgis%type_pkg[version]-mac-grass | qgis%type_pkg[version]-mac-pgsql-grass, 
  r-base (>=2.8.0-1), rpy2-py26 (>=2.1.0) | rpy2-py27 (>=2.1.0)
<<
CompileScript: <<
  #!/bin/sh -ev
  echo "Nothing to compile" 
<<
InstallScript: <<
  #!/bin/sh -ev
  pushd ..
  case %n in
    qgis16-plugin-manager)
      mkdir -p  %i/Applications/QGIS.app/Contents/Resources/python/plugins
      cp -R manageR %i/Applications/QGIS.app/Contents/Resources/python/plugins/
      ;;
    qgis-plugin-manager)
      mkdir -p  %i/Applications/qgis1.5.0.app/Contents/MacOS/share/qgis/python/plugins
      cp -R manageR %i/Applications/qgis1.5.0.app/Contents/MacOS/share/qgis/python/plugins/
      ;;
  esac
<<
