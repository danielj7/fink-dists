 mkdir -p %i/share/doc/twisted2-mail-py%type_pkg[python]
 cp -R doc %i/share/doc/twisted2-mail-py%type_pkg[python]/doc
mv %i/bin/mailmail %i/bin/mailmail%type_raw[python]
<<
