  mv %i/share/man %i/lib/perl5/%type_raw[perl]

  mkdir -p %i/share/%n/examples
  mv %i/bin/*.pl %i/share/%n/examples/
  
  mkdir -p %i/share/%n/cgi
  mv %b/cgi/* %i/share/%n/cgi/
