InstallScript: make -j1 install DESTDIR=%d
PostInstScript: <<
  if [ configure = "$1" ]; then
    export GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source`
    gconftool-2 --makefile-install-rule %p/etc/gconf/schemas/gnome-session.schemas >/dev/null
  fi
