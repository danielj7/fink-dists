PatchScript: <<
#!/bin/sh -ev
	a="x86"
	if [ "%m" == "powerpc" ]; then a="ppc"; fi
	perl -pi -e "s,x86 ppc,$a,;s,QMAKE_MAC_SDK = /Developer/SDKs/MacOSX10.4u.sdk,,;s,10.3,10.4," fotowall.pro
<<
CompileScript: <<
	%p/lib/qt4-mac/bin/qmake fotowall.pro
	make
<<
InstallScript: echo ""
