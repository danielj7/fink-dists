<<
Source: mirror:sourceforge:fink/kdeedu-%vrc5.tar.bz2
Source-MD5: cd708f58f1a38bcf75f7915f66a31222
Source2: mirror:custom:kde-admindir-20021214.tar.bz2
Source2-MD5: c09e25ed693826dfa497d505979e636a
Source2ExtractDir: kdeedu-%vrc5

PatchScript: sed 's|@PREFIX@|%p|g' <%a/%f.patch | patch -p0

SetCFLAGS: -fno-common
SetCPPFLAGS: -no-cpp-precomp -Ddlsym=dlsym_auto_underscore -isystem %p/include -I%p/include
SetLIBS: -L%p/lib
SetMACOSX_DEPLOYMENT_TARGET: 10.2
ConfigureParams: --includedir=%p/include --libdir=%p/lib --prefix=%p --with-qt-dir=%p --with-qt-includes=%p/include/qt --with-extra-libs=%p/lib --with-extra-includes=%p/include --enable-mt --with-pic --enable-rpath --enable-shared=yes --enable-static=no --mandir=%i/share/man --with-xinerama --with-pam --enable-final --disable-dependency-tracking `cat /tmp/kde-build-options`
CompileScript: <<
#!/bin/sh
  export KDEDIR=%p
  make -f Makefile.cvs
  [ $? = 0 ] && sh ./configure %c
  [ $? = 0 ] && make -j2
<<

InstallScript: <<
  make install DESTDIR=%d
  rm -rf %i/share/doc/HTML/en/kmathtool
  mkdir -p %i/share/doc/kde-installed-packages
  touch %i/share/doc/kde-installed-packages/%N
  touch %i/share/doc/kde-installed-packages/%N-base
<<
