License: Artistic
Maintainer: Benjamin Reed <sql-reservedwords-pm@fink.racoonfink.com>

Depends: <<
	fink (>= 0.28.0-1),
	perl%type_pkg[perl]-core,
	sub-exporter-pm%type_pkg[perl]
<<
BuildDepends: <<
	fink (>= 0.28.0-1),
	perl%type_pkg[perl]-core,
	sub-exporter-pm%type_pkg[perl]
<<

