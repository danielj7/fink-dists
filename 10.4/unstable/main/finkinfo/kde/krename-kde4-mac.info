PatchScript: perl -pi -e "s,\#include,\#include <time.h>\n\#include," src/systemplugin.h  && cat %{PatchFile} | patch -p1
CompileScript: <<
#!/bin/sh -ev
	export KDE4_PREFIX=%p KDE4_TYPE=mac
	. %p/sbin/kde4-buildenv.sh
	export PATH=%p/lib/gettext-tools-0.17/bin:%p/lib/mac/bin:%p/lib/qt4-mac/bin:$PATH
	cmake $KDE4_CMAKE_ARGS .
	make
<<
InstallScript: make install DESTDIR=%d
PostInstScript: %p/lib/mac/bin/kde4-postinst.sh >/dev/null 2>&1 || :
