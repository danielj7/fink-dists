CompileScript: <<
#!/bin/sh -ev
# compile rtl and packages for X86-64
  make rtl packages OPT="-ap" CPU_TARGET=x86_64
<<

InstallScript: <<
#!/bin/sh -ev
  make rtl_install packages_install CPU_TARGET=x86_64 INSTALL_PREFIX=%i CROSSINSTALL=1
<<

InfoTest: <<

  TestScript: <<
#!/bin/sh -ev
