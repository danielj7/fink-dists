NoSetCPPFLAGS: true
NoSetLDFLAGS: true
ConfigureParams: --includedir=%p/include --libdir=%p/lib --prefix=%p --with-qt-dir=%p --with-qt-includes=%p/include/qt --with-extra-libs=%p/lib --with-extra-includes=%p/include --enable-mt --with-pic --enable-rpath --enable-shared=yes --enable-static=no --mandir=%i/share/man --with-xinerama --with-pam --disable-final --with-distribution='Fink/Mac OS X' --disable-kig-python-scripting --disable-dependency-tracking
# FIXME: remove --disable-kig-python-scripting when boost has python working

CompileScript: <<
#!/bin/sh -e

  export HOME=/tmp
  export PREFIX="%p"
  . ./environment-helper.sh
  export LDFLAGS="-L%p/lib/python2.4/config"
  export lt_cv_sys_max_cmd_len=65536

  export CC=gcc-3.3 CXX=g++-3.3

  ./build-helper.sh cvs       %N %v %r make -f admin/Makefile.common cvs
  ./build-helper.sh configure %N %v %r ./configure %c
  ./build-helper.sh make      %N %v %r make all all_libraries="$ALL_LIBRARIES"
<<

InstallScript: <<
#!/bin/sh -e

  export PREFIX="%p"
  . ./environment-helper.sh

  ./build-helper.sh install %N %v %r make -j1 install DESTDIR=%d

  mkdir -p %i/share/doc/installed-packages
  touch %i/share/doc/installed-packages/%N
  touch %i/share/doc/installed-packages/%N-base

  rm -rf %i/share/doc/kde/en/kmathtool

  # FIXME: this works around an initialization error in kstars, this should
  # get fixed for real!
  install -d -m 755 %i/share/config
  touch %i/share/config/kstarsrc
<<

