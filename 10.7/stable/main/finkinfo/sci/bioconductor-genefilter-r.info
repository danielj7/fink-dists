Depends: r-base%type_pkg[rversion], bioconductor-biobase-r%type_pkg[rversion], bioconductor-annotationdbi-r%type_pkg[rversion], bioconductor-annotate-r%type_pkg[rversion], cran-survival-r%type_pkg[rversion], libgettext8-shlibs
BuildDepends: r-base%type_pkg[rversion]-dev, libgettext8-dev
PatchScript: <<
  #!/bin/bash -ev
  if [[ %type_raw[rversion] > 2.15 ]]; then
    patch -p1 < %{PatchFile}
  fi
<<
CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  if [[ %type_raw[rversion] > 2.15 ]]; then
    $BIN_R --verbose CMD build --no-build-vignettes genefilter
  else
    $BIN_R --verbose CMD build --no-vignettes genefilter
  fi
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD install --library=%i/lib/R/%type_raw[rversion]/site-library genefilter
  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/genefilter/libs/genefilter.dylib %i/lib/R/%type_raw[rversion]/site-library/genefilter/libs/genefilter.dylib
<<
Shlibs: <<
  %p/lib/R/%type_raw[rversion]/site-library/genefilter/libs/genefilter.dylib 0.0.0 %n (>= 2.0.0-1)
<<
