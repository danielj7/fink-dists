PatchScript: <<
	#!/bin/bash -ev
	darwinvers=`uname -r | cut -f 1 -d .`
	if [ $darwinvers = 10 ]; then
		perl -pi -e 's,\$\^X,/usr/bin/arch -%m \$^X,' t/00-compile.t
	fi
