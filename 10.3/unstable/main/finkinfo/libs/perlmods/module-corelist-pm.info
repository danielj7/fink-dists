Info2: <<
Package: module-corelist-pm%type_pkg[perl]
Version: 1.96
Revision: 2
Type: perl (5.6.0 5.6.1 5.8.0 5.8.1 5.8.4 5.8.5)
Description: What modules shipped with versions of perl
License: Artistic
Homepage: http://search.cpan.org/dist/Module-CoreList/
Maintainer: Blair Zajac <blair@orcaware.com>

# Dependencies.
Depends: module-build-pm%type_pkg[perl]

# Unpack Phase.
Source: mirror:cpan:authors/id/R/RC/RCLAMP/Module-CoreList-%v.tar.gz
Source-MD5: 100645b2857fde6ed7d9361ec4399a32

# Compile Phase.
CompileScript: <<
  perl%type_raw[perl] Build.PL \
    --install_path arch=%i/lib/perl5/%type_raw[perl]/`perl%type_raw[perl] -MConfig -e 'print $Config{archname}'` \
    --install_path bin=%i/bin \
    --install_path bindoc=%i/share/man/man1 \
    --install_path lib=%i/lib/perl5/%type_raw[perl] \
    --install_path libdoc=%i/share/man/man3 \
    --install_path script=%i/bin
  ./Build
  ./Build test
<<

# Install Phase.
UpdatePOD: true
InstallScript: ./Build install
DocFiles: Changes MANIFEST README

SplitOff: <<
  Package: %N-bin
  Description: Binaries for module-corelist-pm
  Depends: %N (= %v-%r)
  Conflicts: module-corelist-pm560-bin, module-corelist-pm561-bin, module-corelist-pm580-bin, module-corelist-pm581-bin, module-corelist-pm584-bin, module-corelist-pm585-bin
  Replaces: module-corelist-pm560-bin, module-corelist-pm561-bin, module-corelist-pm580-bin, module-corelist-pm581-bin, module-corelist-pm584-bin, module-corelist-pm585-bin
  Files: bin
  DocFiles: Changes MANIFEST README
<<

SplitOff2: <<
  Package: %N-man
  Description: Manual pages for module-corelist-pm
  Conflicts: module-corelist-pm560-man, module-corelist-pm561-man, module-corelist-pm580-man, module-corelist-pm581-man, module-corelist-pm584-man, module-corelist-pm585-man
  Replaces: module-corelist-pm560-man, module-corelist-pm561-man, module-corelist-pm580-man, module-corelist-pm581-man, module-corelist-pm584-man, module-corelist-pm585-man
  Files: share/man
  DocFiles: Changes MANIFEST README
<<

# Documentation.
DescDetail: <<
A Perl module that tells when particular modules were first shipped with
Perl.
<<
<<
