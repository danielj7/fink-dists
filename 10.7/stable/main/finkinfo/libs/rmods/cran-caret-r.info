Depends: r-base%type_pkg[rversion], cran-car-r%type_pkg[rversion], cran-foreach-r%type_pkg[rversion],  cran-ggplot2-r%type_pkg[rversion], cran-lattice-r%type_pkg[rversion], cran-plyr-r%type_pkg[rversion], cran-reshape2-r%type_pkg[rversion], libgettext8-shlibs
