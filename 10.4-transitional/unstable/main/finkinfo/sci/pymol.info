PatchScript: sed 's|@FINKPREFIX@|%p|g' <%a/%n.patch | patch -p1
CompileScript: <<
(cd setup; cp Rules.osx-fink %b/Rules.make)
make
<<
InstallScript: <<
mkdir -p %i/bin
mkdir -p %i/lib/%n
(cd setup; cp pymol.com.osx-fink %i/bin/pymol)
cp -r data modules examples test scripts %i/lib/%n
<<
