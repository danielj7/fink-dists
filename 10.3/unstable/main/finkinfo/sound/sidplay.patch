diff -rudN sidplay-base-1.0.9/audio/ao/audiodrv.cpp sidplay-base-1.0.9-bbraun/audio/ao/audiodrv.cpp
--- sidplay-base-1.0.9/audio/ao/audiodrv.cpp	Wed Dec 31 16:00:00 1969
+++ sidplay-base-1.0.9-bbraun/audio/ao/audiodrv.cpp	Tue Feb 10 13:50:48 2004
@@ -0,0 +1,90 @@
+
+#include "audiodrv.h"
+
+static int Initialized = 0;
+static char *up8221to16442(char *inbuf, int insize, int *outsize);
+
+audioDriver::audioDriver()
+{
+	// Reset everything.
+	errorString = "None";
+	frequency = 0;
+	channels = 0;
+	encoding = 0;
+	precision = 0;
+	swapEndian = false;
+}
+
+bool audioDriver::IsThere()
+{
+	return 1;
+}
+
+bool audioDriver::Open(udword inFreq, int inPrecision, int inChannels,
+					   int inFragments, int inFragBase)
+{
+	int driver_id;
+	ao_sample_format afmt;
+
+	if( !Initialized )
+		ao_initialize();
+
+	driver_id = ao_default_driver_id();
+	if( driver_id == -1 ) {
+		errorString = "ERROR: Could not find a usable audo output device";
+		return false;
+	}
+
+	afmt.bits = inPrecision;
+	afmt.rate = inFreq;
+	afmt.channels = inChannels;
+	afmt.byte_format = AO_FMT_NATIVE;
+	adev = ao_open_live(driver_id, &afmt, NULL);
+	if( adev == NULL ) {
+		switch(errno) {
+		case AO_ENODRIVER:
+			errorString = "ERROR: Could not find a usable audo output device";
+			break;
+		case AO_ENOTLIVE:
+			errorString = "ERROR: This driver is not a live output device";
+			break;
+		case AO_EBADOPTION:
+			errorString = "ERROR: A valid libao option key has an invalid value";
+			break;
+		case AO_EOPENDEVICE:
+			errorString = "ERROR: Cannot open output device";
+			break;
+		default:
+			errorString = "ERROR: An unknown libao error occurred";
+			break;
+		}
+
+		return false;
+	}
+	
+	// Transfer input parameters to this object.
+	// May later be replaced with driver defaults.
+	frequency = inFreq;
+	precision = inPrecision;
+	channels = inChannels;
+	fragments = inFragments;
+	fragSizeBase = inFragBase;
+
+	// Assume CPU and soundcard have same endianess.
+	swapEndian = false;
+
+	return true;
+}
+
+// Close an opened audio device, free any allocated buffers and
+// reset any variables that reflect the current state.
+void audioDriver::Close()
+{
+	ao_close(adev);
+	ao_shutdown();
+}
+
+void audioDriver::Play(ubyte* pBuffer, int bufferSize)
+{
+	ao_play(adev, (char *)pBuffer, bufferSize);
+}
