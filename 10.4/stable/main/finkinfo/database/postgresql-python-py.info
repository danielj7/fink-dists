BuildDepends: postgresql80-unified-dev (>= %v-%r), python%type_pkg[python], bison, readline5 (>= 5.0-1004), gcc3.3
Depends: postgresql80-unified-shlibs (>= %v-%r), python%type_pkg[python], python-mx-py%type_pkg[python], readline5-shlibs (>= 5.0-1004)
NoSetCPPFLAGS: true
