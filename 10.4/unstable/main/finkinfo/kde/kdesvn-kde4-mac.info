CompileScript: <<
#!/bin/sh -ev
	export PATH=%p/lib/gettext-tools-0.17/bin:%p/opt/kde4/mac/bin:%p/lib/qt4-mac/bin:$PATH CMAKE_PREFIX_PATH="%p/opt/kde4/mac:%p/opt/soprano/mac"
	cmake -DCMAKE_INSTALL_PREFIX:PATH=%p/opt/kde4/mac -DCMAKE_LIBRARY_PATH:PATH=%p/opt/kde4/mac/lib -DCMAKE_INSTALL_NAME_DIR:PATH=%p/opt/kde4/mac/lib -DSUBVERSION_INCLUDE_DIR:PATH=%p/include/subversion-1 -DSUBVERSION_INSTALL_PATH:PATH=%p .
	make
<<
