Package: commons-pool
Version: 1.3
Revision: 3
Type: java(1.5)
Source: mirror:apache:jakarta/commons/pool/source/%n-%v-src.tar.gz
Source-MD5: a2dcdff75de2af76f5f2169494ed3499
SourceDirectory: %n-%v-src
PatchScript: perl -pi -e 's,source="1.3",source="1.4",g; s,target="1.3",target="1.5",g;' build.xml
BuildDepends: <<
	ant-base (>= 1.6.5-1),
	ant-junit (>= 1.6.5-1),
	commons-collections (>= 3.1-1),
	fink (>= 0.22.2-1),
	fink-mirrors (>= 0.22.1.1-1),
	system-java-dev (>= 1.5-1)
<<
Depends: <<
	commons-collections (>= 3.1-1),
	system-java (>= 1.5-1)
<<
CompileScript: ant dist
JarFiles: dist/*.jar
InstallScript: <<
	mv dist/%n-%v.jar dist/%n.jar
	mkdir -p %i/share/doc/%N
	(cd dist/docs; gnutar -cf - .) | (cd %i/share/doc/%N; gnutar -xf -)
	find %i/share/doc/%N -type d -exec chmod 755 {} \;
	find %i/share/doc/%N -type f -exec chmod 644 {} \;
<<
