Source: http://download.gna.org/kvpnc/kvpnc-%v-kde4.tar.bz2
Source-MD5: 8657957db7252461a7bca6151d151e8e
PatchScript: <<
  grep -rl -- -O2 * |xargs perl -pi -e "s,-O2,,g"
  perl -pi -e "s,\#include \<linux/sockios.h\>,,;s,\#include \<linux/if.h\>,\#include \<net/if.h\>," src/networkinterface.cpp
<<
CompileScript: <<
#!/bin/sh -ev
	export KDE4_PREFIX=%p KDE4_TYPE=mac
	. %p/sbin/kde4-buildenv.sh
	cmake $KDE4_CMAKE_ARGS -DCMAKE_CXX_COMPILER_ARG1:STRING="-D__DARWIN_UNIX03 -I%p/include -L%p/lib" .
	make
<<
InstallScript: make install DESTDIR=%d
