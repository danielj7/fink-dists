SetCXXFLAGS: -MD
GCC: 4.0
CompileScript: <<
	#!/bin/sh -ev
	. %p/sbin/fink-buildenv-kf5.sh

	mkdir build
	pushd build
		cmake \
			$FINK_CMAKE_ARGS \
			$KF5_CMAKE_ARGS \
		..
		make
	popd
	fink-package-precedence --depfile-ext='\.d' .
<<
InstallScript: <<
	#!/bin/sh -ev
	pushd build
		make install/fast DESTDIR=%d
	popd
<<
