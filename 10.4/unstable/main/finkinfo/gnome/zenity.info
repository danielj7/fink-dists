SetCFLAGS: -Os
ConfigureParams: --libexecdir=%p/lib/%n --mandir=%p/share/man --disable-dependency-tracking --disable-scrollkeeper PKG_CONFIG_PATH="%p/lib/pango-ft219/lib/pkgconfig:%p/lib/fontconfig2/lib/pkgconfig:%p/lib/freetype219/lib/pkgconfig:$PKG_CONFIG_PATH" FREETYPE_CONFIG=%p/lib/freetype219/bin/freetype-config
CompileScript: <<
  PERL=/usr/bin/perl ./configure %c
  make
<<
InstallScript: make install DESTDIR=%d
PostInstScript: <<
  if [ configure = "$1" ]; then
    scrollkeeper-update -q
  fi
<<
PostRmScript: <<
  if [ update != "$1" ]; then
    scrollkeeper-update -q
  fi
<<
