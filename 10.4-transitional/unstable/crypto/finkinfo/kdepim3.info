CompileScript: <<
#!/bin/sh -e

  export HOME=/tmp
  export PREFIX="%p"
  . ./environment-helper.sh
  export lt_cv_sys_max_cmd_len=65536
  export ac_cv_path_GPGME_CONFIG="%p/bin/gpgme-config --thread=pth"
  export CC=gcc-3.3 CXX=g++-3.3

  ./build-helper.sh cvs       %N %v %r make -f admin/Makefile.common cvs
  ./build-helper.sh configure %N %v %r ./configure %c

#  perl -pi.bak -e 's,yytext_ptr,libical_ptr,g' libical/src/libical/icallexer.c
#  perl -pi.bak -e 's,yytext_ptr,libicalss_ptr,g' libical/src/libicalss/icalsslexer.c
#  perl -pi.bak -e 's, holidays , ,g' korganizer/plugins/Makefile

  ./build-helper.sh make      %N %v %r make -j1 all all_libraries="$ALL_LIBRARIES" LIB_GNOKII="-lgnokii -lintl -Xlinker -framework -Xlinker CoreFoundation -Xlinker -framework -Xlinker IOBluetooth -Xlinker -framework -Xlinker Foundation"
<<

InstallScript: <<
#!/bin/sh -e

  export PREFIX="%p"
  . ./environment-helper.sh

  ./build-helper.sh install %N %v %r make -j1 install DESTDIR=%d

  mkdir -p %i/share/doc/installed-packages
  touch %i/share/doc/installed-packages/%N
  touch %i/share/doc/installed-packages/%N-base
<<

