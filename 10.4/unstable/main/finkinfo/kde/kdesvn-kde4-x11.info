CompileScript: <<
#!/bin/sh -ev
	export PATH=%p/lib/gettext-tools-0.17/bin:%p/lib/x11/bin:%p/lib/qt4-x11/bin:$PATH
	cmake -DCMAKE_INSTALL_PREFIX:PATH=%p/lib/x11 -DCMAKE_LIBRARY_PATH:PATH=%p/lib/x11/lib -DCMAKE_INSTALL_NAME_DIR:PATH=%p/lib/x11/lib -DSUBVERSION_INSTALL_DIR:PATH=%p .
	make
<<
InstallScript: <<
	make -j1 install DESTDIR=%d
	rm -rf %i/include
	rm -rf %i/lib/x11/lib/libsvnqt.dylib
<<
Shlibs: !%p/lib/x11/lib/libsvnqt.5.dylib
