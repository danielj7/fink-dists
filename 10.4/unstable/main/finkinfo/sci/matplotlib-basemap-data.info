CompileScript:  <<
 #!/bin/sh -ev
 %p/bin/python setup.py build
<<
InstallScript: <<
 #!/bin/sh -ev
 pyversmajor=`python -c 'import sys; print sys.version_info[0]'`
 pyversminor=`python -c 'import sys; print sys.version_info[1]'`
 installpath="lib/python${pyversmajor}.${pyversminor}/site-packages"
 mkdir -p "%i/${installpath}"
 %p/bin/python setup.py install --root %d
<<
