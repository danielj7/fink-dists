CompileScript: <<
 #!/bin/sh -ev
 cd designer-Qt4
 %p/lib/qt4-%type_pkg[qt]/bin/qmake designer.pro
 make
<<
InstallScript: <<
  #!/bin/sh -ev
  cd designer-Qt4
  INSTALL_ROOT=%d make install
<<
