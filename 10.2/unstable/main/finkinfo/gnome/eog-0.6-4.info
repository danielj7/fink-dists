Package: eog
Version: 0.6
Revision: 4
GCC: 3.1
Depends: gconf, gnome-print, bonobo
BuildDepends: gdk-pixbuf, freetype2, libjpeg, libpng, libtiff, netpbm, audiofile, db3 (>= 3.3.11-8), esound, glib, gtk+, orbit, readline (>=4.3-5), giflib, imlib, bonobo-dev, gconf-dev, gnome-libs-dev, gnome-print-dev, oaf-dev, guile-dev (>= 1.4-5), libxml, popt, libglade
Source: mirror:gnome:sources/%n/0.6/%n-%v.tar.gz
Source-MD5: e843959ff17f847e27a3219ac6cafcc7
Patch: %f.patch
UpdateLibtool: true
UpdatePoMakefile: true
SetCPPFLAGS: -no-cpp-precomp
InstallScript: <<
make install DESTDIR=%d
mv %i/bin/eog %i/bin/eog-real
echo '#!/bin/sh' >%i/bin/eog
echo 'exec eog-real --no-xshm "$@"' >>%i/bin/eog
chmod 755 %i/bin/eog
<<
