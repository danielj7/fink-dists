ConfigureParams: --with-plugins=all --disable-schemas-install --mandir=%p/share/man --disable-dependency-tracking PKG_CONFIG_PATH="%p/lib/fontconfig2/lib/pkgconfig:$PKG_CONFIG_PATH" --enable-python PYTHON=%p/bin/python2.6
InstallScript: <<
	make -j1 install DESTDIR="%d"
<<
PostInstScript: <<
	. %p/bin/init.sh
	if [ configure = "$1" ]; then
		export GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source`
		gconftool-2 --makefile-install-rule %p/etc/gconf/schemas/gedit-show-tabbar-plugin.schemas >/dev/null
	fi
 [ -x %p/bin/update-desktop-database ] && update-desktop-database -q || :
<<
PostRmScript: <<
	. %p/bin/init.sh
 [ -x %p/bin/update-desktop-database ] && update-desktop-database -q || :
<<
