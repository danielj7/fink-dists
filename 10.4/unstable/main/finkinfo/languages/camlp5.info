Source-MD5: 61f617f11eedb26ad8607fe21413f920
Depends: ocaml (>= 3.08)
PatchScript: sed 's|@PREFIX@|%p|g' < %a/%n.patch | patch -p1
CompileScript: <<
  ./configure
  make world.opt
<<
InstallScript: <<
  make install DESTDIR=%d
<<
