PatchFile-MD5: c3e07072879524422c558d5f876d14df
PatchScript: <<
 #!/bin/sh -ev
 /usr/bin/patch -p0 < %{PatchFile}
 /usr/bin/sed -i '.bak' "s:python -c:/usr/bin/python -c:" configure
 /usr/bin/sed -i '.bak' "s:ruby -rrbconfig:/usr/bin/ruby -rrbconfig:" configure
 /usr/bin/sed -i '.bak' "s:-Iinclude:-Iinclude -I/usr/X11R6/include:" config/Makefile.in
 /usr/bin/sed -i '.bak' "s/-lfreetype/\`freetype-config --libs\`/" config/root-config.in
 /usr/bin/sed -i '.bak' 's/$(notdir $(@:.dll=.$(SOEXT)))/$(@:.dll=.$(SOEXT))/' cint/cintdlls.mk
 if [ "%type_raw[-qt]" == "-qt" ] ; then 
   /usr/bin/sed -E -i '.bak' "s/(^Gui.*)native/\1qt/g" config/rootrc.in
   /bin/rm config/rootrc.in.bak
 fi
<<
