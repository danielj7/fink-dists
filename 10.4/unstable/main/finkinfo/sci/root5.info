InfoTest: <<
 TestSuiteSize: medium
 TestScript: <<
  #!/bin/sh -ev
  export ROOTSYS=%b
  export DYLD_LIBRARY_PATH=${ROOTSYS}/lib
  export PATH=${ROOTSYS}/bin:${PATH}
  make install DESTDIR=%b
  cd test
  # Patch test Makefile to find includes in build directory
  /usr/bin/sed -i.bak 's:@rootcint -f $@ -c:@rootcint -f $@ -c -I../include -I../cint/include -I../cint/lib -I../cint/stl:g' Makefile
  # Further tweaks to find all the needed stuff in the build directory
  /bin/cp ../sw/etc/root/system.rootrc .rootrc
  /usr/bin/sed -i.bak 's:/sw:%b/sw:g' .rootrc
  /bin/ln -s ../sw/etc/root/system.rootmap .rootmap
  /bin/ln -s ../cint/include/bool.h .
  make ROOTCONFIG="root-config --prefix=%b" || exit 2
  ./hsimple || exit 2
  /bin/ln -s ../test/hsimple.root ../tutorials/
  ./stress || exit 2
  ./stressGeometry || exit 2
  ./stressShapes || exit 2
  ./stressVector || exit 2
  ./stressFit || exit 2
  ./stressGraphics || exit 2
  ./stressLinear || exit 2
  ./stressSpectrum || exit 2
  ./stressEntryList || exit 2
  # Cleanup
  make distclean
  /bin/mv -f Makefile.bak Makefile
  /bin/rm -f bool.h .rootrc .rootrc.bak .rootmap ../tutorials/hsimple.root threads.o
  /bin/rm -f *TrackMathCoreCint* sg*
 <<
<<
