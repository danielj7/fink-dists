Info2: <<
Package: qscintilla2-designer-qt4-%type_pkg[qt]
Version: 2.4.6
Revision: 1
Type: qt (mac x11)
GCC: 4.0
Source: http://www.riverbankcomputing.com/static/Downloads/QScintilla2/QScintilla-gpl-%v.tar.gz
Source-MD5: 424a89736b900e6ca5bc6d929cdafaf8
BuildDepends: <<
  ( %type_pkg[qt] = mac) libpng14,
  ( %type_pkg[qt] = mac) system-openssl-dev,
  libiconv-dev,
  qscintilla2-qt4-%type_pkg[qt] (>= %v-1),
  qt4-%type_pkg[qt] (>= 4.6.3-1)
<<
Depends: <<
  ( %type_pkg[qt] = mac) libpng14-shlibs,
  libiconv,
  qscintilla2-qt4-%type_pkg[qt]-shlibs (>= %v-1),
  qt4-%type_pkg[qt]-core-shlibs (>= 4.6.3-1),
  qt4-%type_pkg[qt]-designer-shlibs (>= 4.6.3-1),
  qt4-%type_pkg[qt]-gui-shlibs (>= 4.6.3-1)
<<

BuildDependsOnly: true
CompileScript: <<
 #!/bin/sh -ev
 cd designer-Qt4
 %p/lib/qt4-%type_pkg[qt]/bin/qmake designer.pro
 make
<<
InstallScript: <<
  #!/bin/sh -ev
  cd designer-Qt4
  INSTALL_ROOT=%d make install
<<
