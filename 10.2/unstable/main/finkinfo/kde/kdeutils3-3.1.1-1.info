Source-MD5: fd2fef424ffb3de13e4fc8d55b995b73
Source2: mirror:custom:kde-admindir-20030328.tar.bz2
Source2-MD5: 712c543a8d3ca4512dfabe886371df2c
Source2ExtractDir: kdeutils-%v

PatchScript: sed 's|@PREFIX@|%p|g' <%a/%f.patch | patch -p0

SetCFLAGS: -fno-common
SetCPPFLAGS: -no-cpp-precomp -isystem %p/include -I%p/include
SetLIBS: -L%p/lib
NoSetLDFLAGS: true
SetMACOSX_DEPLOYMENT_TARGET: 10.2
ConfigureParams: --includedir=%p/include --libdir=%p/lib --prefix=%p --with-qt-dir=%p --with-qt-includes=%p/include/qt --with-extra-libs=%p/lib --with-extra-includes=%p/include --enable-mt --with-pic --enable-rpath --enable-shared=yes --enable-static=no --mandir=%i/share/man --with-xinerama --with-pam --enable-final
CompileScript: <<
#!/bin/sh
  export KDEDIR=%p
  make -f admin/Makefile.common cvs
  [ $? = 0 ] && sh ./configure %c
  [ $? = 0 ] && make -j2
<<

InstallScript: <<
  make install DESTDIR=%d
  rm -rf %i/share/applnk/System/kdf.desktop
  rm -rf %i/share/applnk/System/kwikdisk.desktop
  rm -rf %i/share/applnk/Utilities/More/KCharSelect.desktop
  rm -rf %i/share/applnk/Settingsmenu/kdepasswd.desktop
  rm -rf %i/share/applnk/Utilities/More/khexedit.desktop
  rm -rf %i/share/applnk/Utilities/More/Kjots.desktop
  rm -rf %i/share/applnk/Utilities/More/ktimer.desktop
  rm -rf %i/share/doc/kde/en/kcontrol/kcmlowbatcrit
  rm -rf %i/share/doc/kde/en/kcontrol/kcmlowbatwarn
  rm -rf %i/share/doc/kde/en/kcontrol/laptop
  rm -rf %i/share/doc/kde/en/kcontrol/powerctrl
  rm -rf %i/share/doc/kde/en/ksim
  mkdir -p %i/share/doc/kde-installed-packages
  touch %i/share/doc/kde-installed-packages/%N
  touch %i/share/doc/kde-installed-packages/%N-base
<<
