SetCFLAGS: -fno-common
SetCPPFLAGS: -no-cpp-precomp -isystem %p/include -I%p/include
SetLIBS: -L%p/lib
SetMACOSX_DEPLOYMENT_TARGET: 10.2
ConfigureParams: --includedir=%p/include --libdir=%p/lib --prefix=%p --with-qt-dir=%p --with-qt-includes=%p/include/qt --with-extra-libs=%p/lib --with-extra-includes=%p/include --enable-mt --with-pic --enable-rpath --enable-shared=yes --enable-static=no --mandir=%i/share/man --with-xinerama --with-pam --enable-final --disable-dependency-tracking
CompileScript: <<
#!/bin/sh
  export KDEDIR=%p
  make -f Makefile.cvs
  [ $? = 0 ] && sh ./configure %c
  [ $? = 0 ] && make -j2
<<

InstallScript: <<
  make install DESTDIR=%d
  rm -rf %i/share/doc/HTML/en/kppp
  mkdir -p %i/share/doc/kde-installed-packages
  touch %i/share/doc/kde-installed-packages/%N
  touch %i/share/doc/kde-installed-packages/%N-base
<<
